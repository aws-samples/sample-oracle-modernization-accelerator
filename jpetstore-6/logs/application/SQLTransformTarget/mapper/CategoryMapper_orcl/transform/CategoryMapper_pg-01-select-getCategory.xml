<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.mybatis.jpetstore.mapper.CategoryMapper">
<!-- 2025-06-20 Amazon Q Developer : Converted Oracle SQL to PostgreSQL compatible SQL -->
<!--
  <cache />
-->
<select id="getCategory" resultType="Category">
    SELECT
      CATEGORY.CATID AS categoryId,
      CATEGORY.NAME,
      CATEGORY.DESCN AS description
    FROM CATEGORY
    WHERE CATEGORY.CATID = #{categoryId}
  </select>
</mapper>
