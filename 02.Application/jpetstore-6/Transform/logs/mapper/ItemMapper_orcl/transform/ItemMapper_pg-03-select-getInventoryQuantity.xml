<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.mybatis.jpetstore.mapper.ItemMapper">

<!-- 2025-05-29 Amazon Q Developer : Converted from Oracle to PostgreSQL -->
<select id="getInventoryQuantity" resultType="int">
    SELECT INVENTORY.QTY AS value
    FROM INVENTORY
    WHERE INVENTORY.ITEMID = #{itemId}
  </select>
</mapper>
