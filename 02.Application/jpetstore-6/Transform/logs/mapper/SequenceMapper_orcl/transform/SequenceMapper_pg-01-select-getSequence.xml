<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.mybatis.jpetstore.mapper.SequenceMapper">
<!-- 2025-05-29 Amazon Q Developer : No conversion needed, SQL is PostgreSQL compatible -->
<select id="getSequence" resultType="Sequence">
    SELECT SEQUENCE.name, SEQUENCE.nextid
    FROM SEQUENCE
    WHERE NAME = #{name}
  </select>
</mapper>
